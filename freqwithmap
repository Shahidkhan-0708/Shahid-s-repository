#include <bits/stdc++.h>
using namespace std;
void freq(int arr[],int n){
    unordered_map<int,int>map;
        for(int i=0;i<n;i++){
            map[arr[i]]++;
          }
          for(auto x: map){
              cout<<x.first <<"-> "<<x.second<<endl;
          }
     
    
 
}

int main() {
	// your code goes here
	int arr[]={3,10,10,3,5,5,6,6,8,8,6,3};
	
	int n=sizeof(arr)/sizeof(arr[0]);
	freq(arr,n);
	return 0;

}
