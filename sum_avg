#include <bits/stdc++.h>
using namespace std;
void sum_avg(int arr[],int n){
int sum=0;
for(int i = 0; i < n; i++){
        sum += arr[i];  // correct sum

    cout<<sum<<endl;
    cout<<"the average is "<<(double)sum/n ;
    
}
int main() {
    int n;
    cin>>n;
    int arr[n];

    for(int i=0;i<n;i++){
    
        cin>>arr[i];
        
   }
   sum_avg(arr,n);
   
   
   
	

}
