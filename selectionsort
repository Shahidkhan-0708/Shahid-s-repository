#include <bits/stdc++.h>
using namespace std;
void selectionsort(int arr[], int n) {
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[j] < arr[i]) {   // ascending order
                swap(arr[j], arr[i]);
            }
        }
    }
}



int main() {
    int arr[] = {44,5,55,77,33};
    int n = sizeof(arr) / sizeof(arr[0]);

    selectionsort(arr, n);

    // Print sorted array
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;

	

}
